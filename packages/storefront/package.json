{
  "name": "@withstorefront/storefront",
  "version": "0.1.0",
  "description": "",
  "license": "FSL-1.1-Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/withstorefront/storefront.git",
    "directory": "packages/storefront"
  },
  "bugs": "https://github.com/withstorefront/storefront/issues",
  "homepage": "https://github.com/withstorefront/storefront",
  "type": "module",
  "author": "withstorefront",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "withstorefront",
    "storefront",
    "e-commerce",
    "ecommerce"
  ],
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json && npm run rename:esm",
    "build": "npm run build:cjs && npm run build:esm",
    "clean": "rimraf dist",
    "rename:esm": "/bin/zsh ./scripts/fix-mjs.sh",
    "prepack": "npm run clean && npm run build"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2"
  }
}
